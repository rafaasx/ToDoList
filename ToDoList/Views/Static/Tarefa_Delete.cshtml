                    {{data.Titulo}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="Descricao" class = "control-label col-md-2">
                Descricao
                </label>
                <div class="col-md-10">
                    {{data.Descricao}}
                </div>
            </div>
    
            <div class="form-group">
                <label for="DataCriacao" class = "control-label col-md-2">
                DataCriacao
                </label>
                <div class="col-md-10">
                    {{data.DataCriacao}}
                </div>
            </div>
    
            <div class="form-group">
